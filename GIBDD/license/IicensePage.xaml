<Page x:Class="GIBDD.license.IicensePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GIBDD.license"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Первыпуск прав">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="0.3*"/>
            <RowDefinition/>
            <RowDefinition Height="0.2*"/>
        </Grid.RowDefinitions>
        <GroupBox Header="Номер документа">
            <WindowsFormsHost Name="IicenseMask" FontSize="20"/>
        </GroupBox>
        <Frame Grid.Row="1" Name="IicenseFrame"/>
        <Button Grid.Row="2" Width="200">Перевыпуск</Button>
    </Grid>
</Page>
